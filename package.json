{
  "name": "do-web",
  "version": "1.0.0",
  "description": "APIs for letsdoretro",
  "private": true,
  "author": "Hemadri Dasari",
  "main": "index.ts",
  "scripts": {
    "test": "mocha -S --ui bdd --require ./tests/spec_helper.js --recursive ./tests --reporter spec",
    "dev": "nodemon --exec ts-node ./src/index.ts",
    "dev:debug": "node --inspect ./src/index.ts",
    "start": "node ./dist/index.js",
    "start:prod": "node ./dist/index.js",
    "build": "tsc",
    "build:watch": "tsc -w",
    "lint:list": "eslint */**/*.js --fix",
    "lint": "eslint */**/*.js --fix",
    "migrate:up": "migrate-mongo up",
    "migrate:down": "migrate-mongo down"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/escalonc/nodejs-api-typescript-starter.git"
  },
  "keywords": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/escalonc/nodejs-api-typescript-starter/issues"
  },
  "homepage": "https://github.com/escalonc/nodejs-api-typescript-starter#readme",
  "dependencies": {
    "@types/ejs": "^3.0.6",
    "@types/nodemailer": "^6.4.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "config": "^3.3.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "ejs": "^3.1.6",
    "express": "^4.16.4",
    "express-validator": "^6.10.0",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.25.3",
    "mongoose": "^5.6.10",
    "morgan": "^1.10.0",
    "nodemailer": "^6.5.0",
    "socket.io": "^3.1.0",
    "xlsx": "^0.16.9"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/config": "0.0.38",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/helmet": "0.0.44",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.10.3",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.12.8",
    "@types/socket.io": "^2.1.13",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.6.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.3",
    "nodemon": "^2.0.3",
    "prettier": "^1.19.1",
